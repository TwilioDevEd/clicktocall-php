{
    "name": "twilio-deved/clicktocall-php",
    "description": "Use Twilio to create SMS alerts so that you never miss a critical issue.",
    "keywords": ["twilio", "SMS", "server", "notifications"],
    "require": {
        "php": "^7.2.5",
        "twilio/sdk": "^6.1.0",
        "vlucas/phpdotenv": "^5.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpunit/phpunit": "^9.1",
        "mockery/mockery": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vendor\\Package\\": "tests"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}
